{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "teams",
      "urls": ["http://localhost:3000/api/teams/"],
      "cacheConfig": {
        "maxSize": 1,
        "maxAge": "2h",
        "timeout": "10s",
        "strategy": "freshness"
      }
    },
    {
      "name": "games",
      "urls": ["http://localhost:3000/api/games/"],
      "cacheConfig": {
        "maxSize": 1,
        "maxAge": "2h",
        "timeout": "10s",
        "strategy": "freshness"
      }
    },
    {
      "name": "selections",
      "urls": ["http://localhost:3000/api/selections/"],
      "cacheConfig": {
        "maxSize": 1,
        "maxAge": "2h",
        "timeout": "10s",
        "strategy": "freshness"
      }
    }
  ]
}
